platform :ios, '9.0'

target 'Rhino Fit' do
  use_frameworks!

  # Pods for Rhino Fit
  pod 'SwiftyJSON'
  pod 'Benchmark'
  # Only until Swift 4.2 is officially supported by Charts
  pod 'Charts', :git => 'https://github.com/jlcanale/Charts', :branch => 'Swift-4.2', :inhibit_warnings => true

  target 'Rhino FitTests' do
    inherit! :search_paths
    # Pods for testing
  end

end
